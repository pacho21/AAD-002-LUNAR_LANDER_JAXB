              <xs:element type="xs:byte" name="dificultad"/>
              <xs:element type="xs:byte" name="modeloNave"/>
              <xs:element type="xs:byte" name="modeloLuna"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>